{
  "name": "express-typescript-boilerplate",
  "description": "A boilerplate project for quickly building RESTful APIs using Node.js, Express, PostgreSQL.",
  "author": "sxip",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "serve:prod": "yarn build && yarn start",
    "serve:dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ext .ts,.js . --ignore-path .gitignore .",
    "schema:sync": "cross-env ts-node-dev ./node_modules/typeorm/cli.js schema:sync -f ormconfig.ts",
    "migration:run": "cross-env ts-node-dev ./node_modules/typeorm/cli.js migration:run -f ormconfig.ts",
    "migration:revert": "cross-env ts-node-dev ./node_modules/typeorm/cli.js migration:revert -f ormconfig.ts",
    "seed": "ts-node-dev ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "argon2": "^0.27.1",
    "body-parser": "^1.19.0",
    "bull": "^3.20.0",
    "celebrate": "^13.0.4",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fingerprint": "^1.2.2",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.3.1",
    "ioredis": "^4.19.4",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "rate-limit-redis": "^2.0.0",
    "swig-email-templates": "^6.0.0",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-seeding": "^1.6.1",
    "verror": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bull": "^3.14.4",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.167",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@types/nodemailer": "^6.4.0",
    "@types/rate-limit-redis": "^1.7.1",
    "@types/swig-email-templates": "^5.0.0",
    "@types/verror": "^1.10.4",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "nodemon": "^2.0.6",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
